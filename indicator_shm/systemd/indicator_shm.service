[Unit]
Description=Indicator SHM
DefaultDependencies=no
After=wm_ready.service

[Service]
User=app
Group=app
SmackProcessLabel=org.tizen.indicator_shm
Type=oneshot
RemainAfterExit=yes
ExecStart=/opt/usr/apps/org.tizen.indicator_shm/bin/indicator_shm
EnvironmentFile=/run/tizen-mobile-env

[Install]
WantedBy=multi-user.target
